<UserControl x:Class="print_quotes_calculator.View.QuotesView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:print_quotes_calculator.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <WrapPanel Margin="0,35,0,0">
            <StackPanel Width="110">
                <Label Content="Material Type"/>
            </StackPanel>
            <StackPanel Width="110">
                <Label Content="Material Usage"/>
            </StackPanel>
            <StackPanel Width="110">
                <Label Content="Ink Type"/>
            </StackPanel>
            <StackPanel Width="110">
                <Label Content="Ink Usage"/>
            </StackPanel>
            <StackPanel Width="230">
                <Label Content="Description"/>
            </StackPanel>
            <StackPanel Width="110">
                <Label Content="Cost"/>
            </StackPanel>
        </WrapPanel>
        <ToolBar HorizontalAlignment="Center" Height="30" VerticalAlignment="Top" Width="800"/>

    </Grid>
</UserControl>
